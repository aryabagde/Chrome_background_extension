{
    "manifest_version": 2,
    "name": "DiffBackground",
    "description": "Adding a custom background to google pages",
    "version": "1.0.0",
    "icons": {
        "16": "images/cshield16.png",
        "64": "images/cshield64.png",
        "128": "images/cshield128.png"
    },
    "page_action": {
        "default_icon": {
            "16":"images/cshield16.png",
            "64": "images/cshield64.png",
            "128": "images/cshield128.png"
        }
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "css": ["main.css"]
    }]
}